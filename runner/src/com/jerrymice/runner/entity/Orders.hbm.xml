<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bch.entity.Orders" table="orders" schema="runner">
        <id name="orderNumber">
            <column name="orderNumber" sql-type="int(11)"/>
        </id>
        <property name="money">
            <column name="money" sql-type="decimal(10)" precision="10"/>
        </property>
        <property name="receipt">
            <column name="receipt" sql-type="varchar(256)" length="256"/>
        </property>
        <property name="delivery">
            <column name="delivery" sql-type="varchar(256)" length="256"/>
        </property>
        <property name="remarks">
            <column name="remarks" sql-type="varchar(256)" length="256" not-null="true"/>
        </property>
        <property name="invoiceTime">
            <column name="invoiceTime" sql-type="datetime"/>
        </property>
        <property name="receiptTime">
            <column name="receiptTime" sql-type="datetime"/>
        </property>
        <property name="successTime">
            <column name="successTime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int(10)"/>
        </property>
        <property name="deliveryTime">
            <column name="deliveryTime" sql-type="datetime" not-null="true"/>
        </property>
        <property name="invoiceId">
            <column name="invoiceId" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="receiptId">
            <column name="receiptId" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="size">
            <column name="size" sql-type="int(11)"/>
        </property>
        <property name="coordinate">
            <column name="coordinate" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="itemNumber">
            <column name="itemNumber" sql-type="varchar(20)" length="20"/>
        </property>
    </class>
</hibernate-mapping>