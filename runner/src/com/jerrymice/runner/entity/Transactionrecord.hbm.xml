<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bch.entity.Transactionrecord" table="transactionrecord" schema="runner">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="money">
            <column name="money" sql-type="decimal(10)" precision="10"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int(11)"/>
        </property>
        <property name="uId">
            <column name="uId" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="times">
            <column name="times" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>